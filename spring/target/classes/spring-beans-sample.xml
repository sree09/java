<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<!-- <bean/> definitions here -->
<bean id="carbean" class="factory.Car">
	<property name="wheels" value="4"/>
	<property name="VIN" value="AA123BB564"/>
	<property name="make" value="Honda"/>
</bean>
<bean id="bikebean" class="factory.Bike">
	<property name="wheels" value="2"/>
	<property name="VIN" value="AA123BB654"/>
	<property name="make" value="Hero"/>
</bean>
</beans>